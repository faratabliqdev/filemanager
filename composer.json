  "keywords": [
    "symfony",
    "bundle",
    "file manager"
  ],
  "homepage": "https://github.com/Adsign/FileManagerBundle",
  "license": "MIT",
  "authors": [
    {
      "name": "Ahmad Dehqanian",
      "email": "ahmad.dehqanian@gmail.com"

    },
    {
      "name": "Arsalan Ahadian",
      "email": "Arsalan13812002@gmail.com"
    }
  ],
  "require": {
    "php": ">=5.6.0",
    "sensio/framework-extra-bundle": "~2.3|~3.0,>=3.0.2|4.0.x|~5.0",
    "symfony/translation": "~2.3|~3.0|^4.0",
    "twig/extensions": "~1.0",
    "symfony/form": "~2.3|~3.0|^4.0",
    "symfony/validator": "~2.3|~3.0|^4.0",
    "symfony/twig-bundle": "^2.3.4|~3.0|^4.0",
    "symfony/twig-bridge": "^2.3.4|~3.0|^4.0",
