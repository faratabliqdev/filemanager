    "symfony/browser-kit": "~2.3|~3.0|^4.0",
    "symfony/var-dumper": "~2.3|~3.0|^4.0",
    "symfony/yaml": "~2.3|~3.0|^4.0",
    "symfony/templating":  "~2.3|~3.0|^4.0"
  },
  "autoload": {
    "psr-4": {
